Table LogEntry {
  id int [pk]
  content_type int [ref: > ContentType.id]
  user int [ref: > Usuario.id]
  action_flag smallint
  action_time datetime
  change_message text
  object_id text
  object_repr varchar
}

Table Permission {
  id int [pk]
  content_type int [ref: > ContentType.id]
  codename varchar
  name varchar
}

Table Group {
  id int [pk]
  name varchar
}

Table ContentType {
  id int [pk]
  app_label varchar
  model varchar
}

Table AbstractBaseSession {
  expire_date datetime
  session_data text
}

Table Session {
  session_key varchar
  expire_date datetime
  session_data text
}

Table AbstractUserSocialAuth {
  user int [ref: > Usuario.id]
  created datetime
  extra_data json
  modified datetime
  provider varchar
  uid varchar
}

Table UserSocialAuth {
  id bigint [pk]
  user int [ref: > Usuario.id]
  created datetime
  extra_data json
  modified datetime
  provider varchar
  uid varchar
}

Table Nonce {
  id bigint [pk]
  salt varchar
  server_url varchar
  timestamp int
}

Table Association {
  id bigint [pk]
  assoc_type varchar
  handle varchar
  issued int
  lifetime int
  secret varchar
  server_url varchar
}

Table Code {
  id bigint [pk]
  code varchar
  email varchar
  timestamp datetime
  verified boolean
}

Table Partial {
  id bigint [pk]
  backend varchar
  data json
  next_step smallint
  timestamp datetime
  token varchar
}

Table PermissionsMixin {
  is_superuser boolean
}

Table AbstractBaseUser {
  last_login datetime
  password varchar
}

Table Auditoria {
  id bigint [pk]
  usuario int [ref: > Usuario.id]
  accion varchar
  registro_afectado text
  tabla_afectada varchar
  timestamp datetime
  username_intentado varchar
}

Table Rol {
  id bigint [pk]
  grupo varchar
  descripcion text
  nombre varchar
}

Table Usuario {
  id bigint [pk]
  empresa int [ref: > Empresa.id]
  rol int [ref: > Rol.id]
  sucursal_actual int [ref: > Sucursal.id]
  activo boolean
  direccion text
  email varchar
  fecha_creacion datetime
  foto varchar
  idioma varchar
  is_active boolean
  is_staff boolean
  is_superuser boolean
  last_login datetime
  mfa_enabled boolean
  mfa_secret varchar
  password varchar
  telefono varchar
  tema varchar
  username varchar
}

Table ClaveSATUnidad {
  id bigint [pk]
  clave varchar
  descripcion varchar
}

Table ClaveSATProducto {
  id bigint [pk]
  unidad int [ref: > ClaveSATUnidad.id]
  clave varchar
  descripcion varchar
}

Table Categoria {
  id bigint [pk]
  empresa int [ref: > Empresa.id]
  descripcion text
  nombre varchar
}

Table Producto {
  id bigint [pk]
  categoria int [ref: > Categoria.id]
  clave_sat int [ref: > ClaveSATProducto.id]
  empresa int [ref: > Empresa.id]
  unidad_medida int [ref: > ClaveSATUnidad.id]
  activo boolean
  codigo varchar
  descripcion text
  nombre varchar
  precio_compra decimal
  precio_venta decimal
  stock_minimo decimal
}

Table Inventario {
  id bigint [pk]
  producto int [ref: > Producto.id]
  sucursal int [ref: > Sucursal.id]
  cantidad decimal
  fecha_vencimiento varchar
  lote varchar
}

Table MovimientoInventario {
  id bigint [pk]
  inventario int [ref: > Inventario.id]
  usuario int [ref: > Usuario.id]
  cantidad decimal
  fecha datetime
  tipo_movimiento varchar
}

Table Cliente {
  id bigint [pk]
  empresa int [ref: > Empresa.id]
  actualizado_en datetime
  apellido_materno varchar
  apellido_paterno varchar
  correo varchar
  creado_en datetime
  direccion_calle varchar
  direccion_codigo_postal varchar
  direccion_colonia varchar
  direccion_estado varchar
  direccion_municipio varchar
