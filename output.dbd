}

Table PagoCompra {
  id bigint [pk]
  compra int [ref: > Compra.id]
  usuario int [ref: > Usuario.id]
  actualizado_en datetime
  creado_en datetime
  fecha_pago datetime
  metodo_pago varchar
  monto decimal
  nota text
}

